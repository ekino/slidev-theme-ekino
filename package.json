{
  "name": "@ekino/slidev-theme-ekino",
  "version": "1.2.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "ekino"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "52.1.0"
  },
  "devDependencies": {
    "@slidev/cli": "52.1.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "fonts": {
        "sans": "Graphik"
      }
    }
  }
}
